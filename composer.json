        "spectrefleet/services": "^3.0",
        "itsjavi/bootstrap-colorpicker": "^2.0",
        "s9e/text-formatter": "^1.1"
    },
    "authors": [{
        "name": "Kassie Yvo",
        "email": "kassie.yvo@gmail.com"
    }, {
        "name": "Warlof Tutsimo",
        "email": "loic.leuilliot@gmail.com"
    }],
    "license": "GPL-3.0-or-later",
    "minimum-stability": "alpha",
